{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc": "off",
        "max-len": ["error", 100],
        "strict": ["error", "global"]
    },
    "ignorePatterns": ["public/vendor/**"]
}
